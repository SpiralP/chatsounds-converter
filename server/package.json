{
  "name": "chatsounds-converter-server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "yarn --production=true node -r dotenv/config ./dist/index.js",
    "start:dev": "node -r source-map-support/register -r dotenv/config ./dist/index.js",
    "dev": "yarn run typecheck && yarn run build:dev && yarn run start:dev",
    "build": "yarn --production=true run build:dev",
    "build:dev": "node build.js",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.0.20",
    "dotenv": "^16.0.0",
    "express": "^4.17.1",
    "fluent-ffmpeg": "^2.1.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/fluent-ffmpeg": "^2.1.15",
    "@types/pump": "^1.1.0",
    "@types/tmp": "^0.2.0",
    "pump": "^3.0.0",
    "source-map-support": "^0.5.21",
    "tmp": "^0.2.1",
    "typescript": "^4.0.2"
  }
}
