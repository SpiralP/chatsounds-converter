{
  "name": "chatsounds-converter",
  "private": true,
  "scripts": {
    "start": "yarn workspace chatsounds-converter-server run start",
    "start:dev": "yarn workspace chatsounds-converter-server run start:dev",
    "dev": "yarn run typecheck && yarn run build:dev && yarn run start:dev",
